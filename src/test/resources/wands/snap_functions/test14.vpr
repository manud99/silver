// based on test11 in QPPredicates.vpr
method test14f(x: Ref, z: Ref)
    requires Cell(x)
    requires x != z
{
    package (forall y: Ref :: y != x ==> Cell(y)) --* (forall y: Ref :: true ==> Cell(y))
    inhale forall y: Ref :: y != x ==> Cell(y)

    unfold Cell(z)
    z.f := 0
